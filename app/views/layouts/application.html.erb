
<html>
  <head>
    <title>Homework</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="bg">
  	<nav class="navbar navbar-default ">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand " href="/posts">Social</a>
    </div>
     <%if user_signed_in?%> 
    <ul class="nav navbar-nav">      
      <li><a href="/posts/my_post">My Post</a></li>
      <li><%= link_to "All User", all_users_index_path(params[:email]), method: "GET" %></li>
      <li><%= link_to "Friend request", friendships_path %></li>
      <li><%= link_to "My friends", myfriends_friendships_path %></li>
      <li><%= link_to "Profile", user_profile_post_path(current_user.id) %></li>
    </ul>
    <ul class="nav navbar-nav navbar-right">  
      <li style="margin-top: 17px;"><b><%= current_user.email%></b></li>   
      <li class="active"><%= link_to "Log out", destroy_user_session_path, method: "delete",class: "btn " %></a>
      </li>
    </ul>    
      <%else%>
    <ul class="nav navbar-nav navbar-right">
      <li class="active" ><%= link_to "Log in", new_user_registration_path %></li>
      <%end%>
    </ul>
  </div>
</nav>
    <%= yield %>
  </body>
</html>







